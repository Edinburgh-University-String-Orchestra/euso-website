&lt;figure class=&quot;video-container&quot;&gt;
  &lt;video controls=&quot;true&quot; allowfullscreen=&quot;true&quot; poster=&quot;/66c7ab1045258aeb593d7dad59736597/vlv-poster.png&quot; width=&quot;100%&quot;&gt;
